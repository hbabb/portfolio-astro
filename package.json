{
  "name": "portfolio-astro",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version"
    ]
  },
  "packageManager": "pnpm@10.24.0",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "test:e2e": "playwright test",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "check:updates": "pnpm outdated",
    "update:interactive": "pnpm update --interactive",
    "update:all": "pnpm update --latest",
    "prepare": "husky"
  },
  "dependencies": {
    "@arcjet/astro": "1.0.0-beta.15",
    "@astrojs/markdoc": "^0.15.9",
    "@astrojs/mdx": "^4.3.12",
    "@astrojs/netlify": "^6.6.3",
    "@astrojs/react": "^4.4.2",
    "@astrojs/sitemap": "^3.6.0",
    "@iconify-json/cib": "^1.2.3",
    "@iconify-json/fa7-brands": "^1.2.1",
    "@iconify-json/logos": "^1.2.10",
    "@iconify-json/simple-icons": "^1.2.62",
    "@iconify-json/skill-icons": "^1.2.3",
    "@iconify-json/vscode-icons": "^1.2.37",
    "@marsidev/react-turnstile": "^1.4.0",
    "@sentry/astro": "^10.32.1",
    "@t3-oss/env-core": "^0.13.10",
    "astro": "^5.16.4",
    "astro-icon": "^1.1.5",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "resend": "^6.6.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.5.1",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint-react/eslint-plugin": "^2.3.12",
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.10.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "astro-eslint-parser": "^1.2.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-format": "^1.1.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "prettier-plugin-astro": "^0.14.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  },
  "lint-staged": {
    "*": [
      "pnpm format",
      "pnpm lint:fix"
    ],
    "ignore": [
      ".pnpm-store/**",
      "dist/**",
      "node_modules/**"
    ]
  }
}
